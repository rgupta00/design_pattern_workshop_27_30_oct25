class StackImpUsingArray {
	private int x[];
	private final int SIZE;
	private int top;
	
	public StackImpUsingArray(int size) {
		this.SIZE=size;
		top=-1;
		
	}
	
	
	public void push(int element){
		if(top>=SIZE)
			return ;
		else
			x[top++]=element;
	}
	
	
	public int pop(){
		if(top==-1)
			return -99;
		else
			return x[top--];
	}
}
